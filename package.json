  "license": "MIT",
  "dependencies": {
    "@rondinif/phytojs": "^1.3.0",
    "esm": "^3.2.25",
    "isomorphic-fetch": "^2.2.1"
  },
  "keywords": [
    "origano",
    "linked data",
    "plants",
    "rdf",
    "spqrql",
    "vegetables",
    "vegetable garden",
    "wikidata",
    "phytojs"
  ],
  "homepage": "https://github.com/9llara/phytojs-minimal-node-esm#readme",
